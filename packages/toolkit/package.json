{
  "name": "@contentauth/toolkit",
  "version": "0.10.0",
  "author": {
    "name": "Dave Kozma",
    "email": "dkozma@adobe.com"
  },
  "license": "MIT",
  "description": "WebAssembly binary and bindings for the c2pa-rs library",
  "type": "module",
  "main": "pkg/toolkit.js",
  "types": "pkg/toolkit.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/contentauth/c2pa-js",
    "directory": "packages/toolkit"
  },
  "files": [
    "pkg/**/*"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "dev": "nodemon ./scripts/build.js -e rs -w src -w ../../../c2pa-toolkit/lib -- --dev --verbose",
    "build": "rushx clean && node ./scripts/build.js",
    "build:release": "rushx build",
    "build:verbose": "rushx clean && node ./scripts/build.js --verbose",
    "build:types": "tsc",
    "clean": "rimraf ./pkg"
  },
  "devDependencies": {
    "@contentauth/testing": "workspace:*",
    "@rollup/plugin-replace": "~5.0.2",
    "@types/jasmine": "~4.0.3",
    "@web/dev-server-esbuild": "~0.3.6",
    "@web/dev-server-rollup": "~0.3.21",
    "@web/test-runner": "~0.13.31",
    "@web/test-runner-browserstack": "~0.5.1",
    "@web/test-runner-core": "~0.10.29",
    "@web/test-runner-puppeteer": "~0.10.5",
    "debug": "~4.3.4",
    "deepdash-es": "~5.3.9",
    "fast-glob": "~3.2.12",
    "jasmine-core": "~4.1.1",
    "nodemon": "~2.0.22",
    "p-reduce": "~3.0.0",
    "pkg-dir": "~7.0.0",
    "prettier": "^2.8.8",
    "rimraf": "^4.4.1",
    "rollup": "~2.70.2",
    "ssri": "~10.0.4",
    "typescript": "~4.6.4",
    "web-test-runner-jasmine": "~0.0.5"
  }
}
